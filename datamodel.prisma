enum Permission {
  ADMIN
  USER
  PERMISSIONUPDATE
}

enum TransportMode {
  TRANSIT
  BIKE
  CAR
  WALK
}

enum RoutePreference {
  OPTIMAL
  LESS_TRANSFER
  LESS_WALKING
  CHEAP
  SAFE
  BETTER_FOR_WALK
}

type User {
  id: ID! @unique
  name: String!
  email: String! @unique
  password: String!
  resetToken: String
  resetTokenExpiry: Float
  permissions: [Permission]
  preferMode: TransportMode
  preferRoute: RoutePreference
}
